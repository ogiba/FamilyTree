<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>{% block title %}{{ trans("app-name") }}{% endblock %}</title>
    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Tangerine" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="{{ asset('stylesheets/bootstrap.min.css') }}"/>
    {% block stylesheets %}
        <link rel="stylesheet" type="text/css" href="{{ asset('stylesheets/base.css') }}"/>
    {% endblock %}
    {#<link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />#}
</head>
<body>
{% block header %}{% endblock %}
{% block body %}{% endblock %}
{% block javascripts %}
    <script src="{{ asset('scripts/plugins/jquery-3.2.1.min.js') }}"></script>
    <script src="{{ asset('scripts/nav_bar.js') }}"></script>
    <script src="{{ asset('scripts/plugins/bootstrap.min.js') }}"></script>
{% endblock %}
</body>
{% block footer %}
    <footer>
        <div>
            <div class="row">
                <div class="col-md-6">
                    <div class="row">
                        <div class="col-md-12">
                            <a href="/">{{ trans("home-page") }}</a>
                        </div>
                        <div class="col-md-12">
                            <a href="/tree">{{ trans("tree-page") }}</a>
                        </div>
                    </div>
                </div>
                <div class="col-md-6"></div>
            </div>
        </div>
        <div>
            Created by Robert Ogiba
        </div>
    </footer>
{% endblock %}
</html>