{% extends 'base.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    <link href="{{ asset('stylesheets/tree_list.css') }}" type="text/css" rel="stylesheet"/>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('scripts/plugins/jquery-ui.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('scripts/plugins/jsrender.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('scripts/plugins/bootstrap.min.js') }}" type="text/javascript"></script>
{% endblock %}

{% block header %}
    <header>
        {% embed 'default/nav_bar.html.twig' %}
        {% endembed %}
    </header>
{% endblock %}

{% block body %}
    <section class="trees-section">
        <div class="container">
            <div class="row">
                {% for i in 1..10 %}
                    <div class="col-md-4">
                        {% include 'trees/templates/tree_list_item.html.twig' with { "family" : families[0]} %}
                    </div>
                {% endfor %}
            </div>
        </div>
    </section>
{% endblock %}

{% block footer %}
{% endblock %}
